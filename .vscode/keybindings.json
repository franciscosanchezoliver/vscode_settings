// Place your key bindings in this file to override the defaultsauto[]
[
    // test at cursor
    {"key": "ctrl+t", "command": "testing.debugAtCursor"},

    // Execute code selected in debug console
    {
        "key": "ctrl+oem_period",
        "command": "editor.debug.action.selectionToRepl"
    },
    {
        "key": "ctrl+alt+p",
        "command": "git.push"
    },
    // hide/show terminal
    {
        "key":"ctrl+alt+m",
        "command":"workbench.action.togglePanel"
    }
]
